<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapp.myapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AllEvents"
            android:label="Calendar-All Events" />

        <activity
            android:name=".EditEvents"
            android:label="Calendar-Edit/Delete Event" />
        <activity
            android:name=".NewEvent"
            android:label="Calendar-New Event" />

        <activity
            android:name=".ImportFrom"
            android:label="Calendar-Import From Device" />
        <activity
            android:name=".DeviceEvents"
            android:label="Import Device Events" />
        <activity
            android:name=".eachDeviceEventDisplay"
            android:label="Device Event" />
        <activity
            android:name=".onSelect"
            android:label="Event Display" />

        <activity
            android:name=".ExportTo"
            android:label="Device Event Export" />

        <activity
            android:name=".aboutUs"
            android:label="Creators" />


        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
